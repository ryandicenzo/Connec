{
  "expo": {
    "name": "Connec",
    "description": "Connec lets you instantly share your contact information and social media profile with anyone.",
    "slug": "connec_v1",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dicenzo.connec",
      "infoPlist": {
        "NSCameraUsageDescription": "Connec uses the camera to scan QR codes."
      }
    },
    "android": {
      "package": "com.dicenzo.connec",
      "versionCode": 3
    }
  }
}
